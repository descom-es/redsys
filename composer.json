{
    "name": "descom/redsys",
    "description": "A PHP package to platform Redsys",
    "type": "library",
    "version": "1.8.3",
    "require": {
        "php": "^8.2",
        "php-http/discovery": "^1.17",
        "psr/http-client-implementation": "*",
        "psr/http-factory-implementation": "*"
    },
    "homepage": "https://github.com/descom/redsys",
    "license": "MIT",
    "authors": [
        {
            "name": "Cesar Garcia",
            "email": "cesar@descom.es",
            "role": "Developer"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "illuminate/http": "^11.20",
        "nyholm/psr7": "^1.8",
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^9.3",
        "symfony/http-client": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Descom\\Redsys\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Descom\\Redsys\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --stop-on-error --colors=always",
        "style": "vendor/bin/php-cs-fixer fix",
        "analyse": "vendor/bin/phpstan analyse"
    }
}
